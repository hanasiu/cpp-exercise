#include <string>
#include <iostream>

using namespace std;
struct Person {
	Person() = default;
	Person(const string &s) : name(s) { }
	Person(const string &n, const string &b) : name(n), address(b) { }
	Person(istream&);

	std::string getName() const { return name; }
	std::string getAddress() const { return address; }
	
	std::string name;
	std::string address;
};

std::istream& read(std::istream& is, Person& rhs) {
	is >> rhs.name >> rhs.address;
	return is;
}

std::ostream& print(std::ostream& os, const Person& rhs) {
	os << rhs.getName() << " " << rhs.getAddress();
	return os;
}
Person::Person(istream& is) {
	read(is, *this);
}
int main() {
	Person p1;
	Person p2("김유");
	Person p3("김유", "견훤로");
	Person p4(cin);
//	read(std::cin, p1);
//	print(std::cout, p1) << std::endl;
	cout << p1.name << " " << p1.address << endl
		<< p2.name << " " << p2.address << endl
		<< p3.name << " " << p3.address << endl
		<< p4.name << " " << p4.address << endl;
	return 0;
}
